{
  "manifest_version": 3,
  "name": "Editor Content Logger",
  "version": "1.0",
  "permissions": [
    "scripting",
    "activeTab",
    "tabs",
    "webRequest"  
  ],
  "host_permissions": [
    "https://*.geeksforgeeks.org/*",
    "https://*.codechef.com/*"
  ],"web_accessible_resources": [
    {
      "resources": [ "script/getSolCf.js", "script/getSolGfg.js" ],
      "matches": [ "<all_urls>" ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  }
}